        android:supportsRtl="true"
        android:theme="@style/DefaultTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning">
        <activity
            android:name=".SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login"/>
        <activity android:name=".Pengaturan"/>
        <activity android:name=".MainActivity" />
        <activity android:name=".Input"/>
        <activity android:name=".MulaiActivity"/>
        <activity android:name=".input2"/>
        <activity android:name=".Pilihan"/>
        <activity android:name=".Daftar"/>
        <activity android:name=".View_PDF_Files"/>
        <activity android:name=".UploadActivity"/>
        <activity android:name=".ImageActivity"/>
        <activity android:name=".ImagesActivity"/>
        <activity android:name=".Slide"/>
    </application>

</manifest>